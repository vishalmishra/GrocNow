<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    package="com.dev1.vishalmishra.grocnow">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:name=".BaseRealmApplication"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launch"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme"
        tools:replace="android:icon">
        <activity android:name=".MainActivity">
            <intent-filter>
                <category android:name="android.intent.category.ALTERNATIVE" />
            </intent-filter>
        </activity>
        <activity
            android:name=".myprofile"
            android:label="My Profile" />
        <activity android:name=".Login">
            <intent-filter>
                <category android:name="android.intent.category.DEFAULT" />
            </intent-filter>
        </activity>
        <activity android:name=".Signup" />
        <activity android:name=".Launcher">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".test"
            android:label="Products"/>
        <activity android:name=".product_list" />
        <activity android:name=".mycart"
            android:label="My Cart"/>
        <activity android:name=".category"
            android:label="Categories"/>
        <activity
            android:name=".search"
            android:label="" />
        <activity android:name=".Continue_checkout" />
        <activity android:name=".myorders"
            android:label="My Orders"></activity>
    </application>

</manifest>